## Alias - short for "at another time or place".

# Reload the config
alias reload="source ${ZDOTDIR}/.zshrc"
alias zshrc="nvim ${ZDOTDIR}"

# true color tmux
alias tmux='TERM=screen-256color tmux'

# Very short aliases for most used commands.
alias n="nvim"
alias t="tmux"
alias o="open"
